{
  "name": "wordpress-marbles",
  "version": "1.0.0",
  "description": "Functional Reactive Wordpress Adapter with Marble.js",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "docker": "docker-compose -f .docker/docker-compose.yml up -d",
    "start": "ts-node --project tsconfig.json src/index.ts",
    "watch": "nodemon --watch src --ext ts --ignore '*.spec.ts' --exec npm run start:dev"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/jsonberry/wordpress-marbles"
  },
  "keywords": [
    "Functional",
    "Reactive",
    "RxJS",
    "Marble",
    "WordPress",
    "API",
    "Adapter",
    "Pattern"
  ],
  "author": "Jason Awbrey",
  "license": "MIT",
  "dependencies": {
    "@marblejs/core": "^1.2.1",
    "@marblejs/middleware-body": "^1.2.1",
    "@marblejs/middleware-joi": "^1.2.1",
    "@marblejs/middleware-logger": "^1.2.1",
    "@types/node": "^10.12.10",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "injection-js": "^2.2.1",
    "inversify": "^5.0.1",
    "nodemon": "^1.18.6",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "rxjs-toolkit": "^1.1.1",
    "sqlite3": "^4.0.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typeorm": "^0.2.9",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "prettier": "^1.15.2"
  }
}
